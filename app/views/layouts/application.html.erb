<!DOCTYPE html>
<html>
  <head>
    <title>EbamBlog</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <nav class="navbar-inverse navbar-fixed-top">
      <div class="container-fluid">
        <div class="navbar-header">
          <div class="navbar-brand ebiz">
          <%= link_to "e-Business Association Malaysia (Secret Commitee Post)", root_path %>
          </div>
        </div>
        <div id="navbar" class="navbar-collapse collapse">
          <ul class="nav navbar-nav navbar-right">
          </ul>
        </div>
      </div>
    </nav>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>
    <div class="container-fluid">
      <div class="row">
        <div class="col-sm-3 col-md-4 sidebar">
          <ul class="nav nav-sidebar">
            <li class="active"><%= link_to "Overview", root_path %></li>
            <li><%= link_to "Log Out", destroy_user_session_path, :method => :delete %></li>
            <li><%= link_to "Post New Secret", new_post_path %></li>
            <li><a href="web">Website</a></li>
            <li><a href="/fb">Facebook</a></li>
            <li><a href="/melab">Design by MeLAB</a></li>
            <li><%= image_tag "ebam.png" %></li>
          </ul>

        </div>
    <%= yield %>
  </body>
</html>
